@model IEnumerable<GroceryChoice.Model.MinorCategory>

<div class="panel panel-primary">
    <div class="panel-heading">Minor Categories</div>
    <table class="table table-striped">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Major Category ID</th>
            <th></th>
        </tr>
        @foreach (var mc in Model)
        {
            <tr>
                <td><a href="/prep/finditemcategories/@mc.MinorCategoryId">@mc.MinorCategoryId</a></td>
                <td>@mc.MinorCategoryName</td>
                <td>@mc.MajorCategoryId</td>
            </tr>
        }
    </table>
</div>

